bmit = event => {\n        event.preventDefault()\n        let id = props.id\n\n        if (client.lastname.length && client.lastname.length > 0) {\n            if (client.phone === \"\" || !isNaN(client.phone) && client.phone.length === 10) {\n            API.update(id,client)\n                .then(() => props.renderUpdate(id))\n                .catch(err => console.log(err))\n                alert(\"Client mis à jour!\")\n                }\n            else {\n                alert(\"Entrez un numéro de téléphone à 10 chiffres!\")\n                 ReactDOM.render(\n                    <ClientUpdateRow\n                        id={id}\n                        deleteClient={props.deleteClient}\n                        openUpdateForm={props.openUpdateForm}\n                    />, document.getElementById(id));\n                    }\n        \n      } else {\n           alert(\"Entrez un nom (2 caractères au moins) pour enregistrer\")\n             ReactDOM.render(\n                    <ClientUpdateRow\n                        id={id}\n                        deleteClient={props.deleteClient}\n                        openUpdateForm={props.openUpdateForm}\n                    />, document.getElementById(id));\n       }\n  }\n\n    return (\n        <>\n            <td className=\"card-body\">{props.id}</td>\n            <td>\n               <form className=\"search input-group mt-1 mr-2 d-inline\">\n                    <div className=\"input-group\">\n                        <input\n                            name=\"lastname\"\n                            onChange={handleInputChange}\n                            id=\"lastname\"\n                            type=\"text\" className=\"form-control\" defaultValue={props.lastname}\n                        />\n                    </div>\n                </form>\n            </td>\n            <td>\n                <form className=\"search input-group mt-1 mr-2 d-inline\">\n                    <div className=\"input-group\">\n                        <input\n                            name=\"firstname\"\n                            onChange={handleInputChange}\n                            id=\"firstname\"\n                            type=\"text\" className=\"form-control\" defaultValue={props.firstname}\n                        />\n                    </div>\n                </form>\n            </td>\n            <td>\n                <form className=\"search input-group mt-1 mr-2 d-inline\">\n                    <div className=\"input-group\">\n                        <input\n                            name=\"phone\"\n                            onChange={handleInputChange}\n                            id=\"phone\"\n                            type=\"text\" className=\"form-control\" defaultValue={props.phone}\n                            maxLength=\"10\"\n                        />\n                    </div>\n                </form>\n            </td>\n            <td><i id={props.id} onClick={handleSubmit} className=\"fa fa-save\"/></td>\n            <td><i id={props.id} onClick={(() =>  props.renderUpdate(props.id))} className=\"fa fa-close\"/></td>\n        </>\n    )\n}","C:\\Users\\boute\\OneDrive\\Desktop\\liste-clients\\frontend\\src\\components\\ClientUpdateRow\\index.js",[],"C:\\Users\\boute\\OneDrive\\Desktop\\liste-clients\\frontend\\src\\api\\index.js",[],{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":29,"column":61,"nodeType":"64","messageId":"65","endLine":29,"endColumn":63},{"ruleId":"66","severity":1,"message":"67","line":28,"column":37,"nodeType":"68","messageId":"69","endLine":28,"endColumn":39},{"ruleId":"66","severity":1,"message":"67","line":28,"column":61,"nodeType":"68","messageId":"69","endLine":28,"endColumn":63},"no-native-reassign",["70"],"no-negated-in-lhs",["71"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-mixed-operators","Unexpected mix 